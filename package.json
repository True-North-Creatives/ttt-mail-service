{
  "name": "ttt-mailer",
  "version": "1.0.0",
  "description": "mailer service for timettrain web app",
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "dev": "NODE_ENV=dev nodemon --exec babel-node src/index.js",
    "transpile": "babel src -d lib",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "NODE_ENV=staging node lib/index.js",
    "start:prod": "NODE_ENV=prod node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/True-North-Creatives/ttt-mailer.git"
  },
  "keywords": [
    "ttt",
    "mail"
  ],
  "author": "Akhil Reddy Gangula <akhilreddy.gangula96@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/True-North-Creatives/ttt-mailer/issues"
  },
  "homepage": "https://github.com/True-North-Creatives/ttt-mailer#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.7",
    "ttt-packages": "0.0.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  }
}
